{
  "name": "@router402/sdk",
  "version": "0.3.2",
  "description": "TypeScript SDK for Router402 â€” an OpenRouter-compatible AI gateway with micropayment billing via x402",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/itublockchain/hackmoney-router402.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://github.com/itublockchain/hackmoney-router402/tree/master/packages/sdk",
  "bugs": {
    "url": "https://github.com/itublockchain/hackmoney-router402/issues"
  },
  "keywords": [
    "router402",
    "x402",
    "smart-accounts",
    "session-keys",
    "zerodev",
    "kernel",
    "erc-4337",
    "account-abstraction",
    "micropayments",
    "usdc"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node --format esm --external @zerodev/ecdsa-validator --external @zerodev/permissions --external @zerodev/sdk --external permissionless --external viem",
    "build:publish": "bun run build && tsc --emitDeclarationOnly -p tsconfig.build.json",
    "declarations": "tsc --emitDeclarationOnly -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "viem": "^2.0.0"
  },
  "dependencies": {
    "@zerodev/ecdsa-validator": "^5.4.9",
    "@zerodev/permissions": "^5.6.3",
    "@zerodev/sdk": "^5.5.7",
    "permissionless": "^0.3.4"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "viem": "^2.23.7"
  }
}
