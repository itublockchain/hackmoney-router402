{
  "name": "@router402/sdk",
  "version": "0.1.0",
  "description": "Router402 SDK for Kernel smart accounts with session keys",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun && tsc --emitDeclarationOnly -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "viem": "^2.0.0"
  },
  "dependencies": {
    "@zerodev/ecdsa-validator": "^5.4.9",
    "@zerodev/permissions": "^5.6.3",
    "@zerodev/sdk": "^5.5.7",
    "permissionless": "^0.3.4"
  },
  "devDependencies": {
    "typescript": "^5.7.3",
    "viem": "^2.23.7"
  }
}
